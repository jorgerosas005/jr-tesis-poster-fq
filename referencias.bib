@article{Wa-2024-03,
title = {Structures, spectroscopy, binding and clustering energies of the hydrated copper dication clusters},
journal = {Computational and Theoretical Chemistry},
volume = {1236},
pages = {114609},
year = {2024},
issn = {2210-271X},
doi = {https://doi.org/10.1016/j.comptc.2024.114609},
url = {https://www.sciencedirect.com/science/article/pii/S2210271X24001488},
author = {Tabouli Eric Da-yang and Jean Jules Fifen and Zoubeida Dhaouadi and Mama Nsangou and Jeanet Conradie},
keywords = {Binding energies, Clustering energies, Temperature effect, Hydrated copper dication, Coordination number, Spectroscopy, MP2},
abstract = {The hydration-free energy of the Cu2+ cation stands as a crucial parameter for comprehending its reactivity with various molecules. However, the existing literature lacks a definitive consensus on this value. To solve this problem, we use the cluster continuum model to evaluate this parameter accurately, requiring the determination of the energy potential of Cu2+(H2O)n clusters at different temperatures. Our project initiates with an exploration of structures, relative stabilities, infrared spectroscopy, binding energies, and clustering energies of hydrated Copper dication clusters, utilizing the MP2 ab initio method. We propose a unique fit function capable of precisely deriving binding and clustering energies per solvent molecule for the solvated Cu2+ cation in water. Furthermore, we extend our findings to encompass ammonia as a solvent. The binding and clustering energies exhibit minimal changes showcasing remarkable consistency between water and ammonia as solvents. This saturation phenomenon allows us to understand the behavior of solvated ions.}
}

